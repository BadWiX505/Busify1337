{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/api/auth/[...nextauth]/provider.js", "src/app/api/auth/[...nextauth]/provider.js", "src/components/login/binaryuAnimation.jsx", "src/app/server/db.js", "src/Repo/Logic.js", "src/app/api/auth/[...nextauth]/route.js", "src/lib/db.js", "src/lib/db.js", "src/app/login/page.jsx", "src/app/Application/getStarted/page.jsx"],
  "exclude": ["node_modules"]
}
